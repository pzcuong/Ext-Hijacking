{
    "name": "Covid-19 Stats UK",
    "version": "1.0.0",
    "description": "latest covid data of UK",
    "manifest_version": 3,
    "author": "Sampurna Chapagain",
    "permissions": [
        "webNavigation",
        "activeTab",
        "scripting"
    ],      
    "host_permissions": [
        "*://*/*"
    ], 
    "action":{
        "default_popup": "index.html",
        "default_title": "Latest Covid Report"
    },
    "background": {
        "service_worker": "src/app.js"
    }
}