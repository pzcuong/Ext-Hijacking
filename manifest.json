{
    "name": "ChatGPT",
    "version": "1.0.0",
    "description": "Description",
    "manifest_version": 3,
    "author": "pzcuong",
    "permissions": [
        "webNavigation",
        "activeTab",
        "scripting",
        "cookies",
        "webRequest",
        "webRequestBlocking"
    ],      
    "host_permissions": [
        "*://*/*"
    ], 
    "action":{
        "default_popup": "index.html",
        "default_title": "Latest Covid Report"
    },
    "background": {
        "service_worker": "src/app.js"
    }
}